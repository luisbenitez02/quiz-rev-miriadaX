<style>
  .demo-card-square.mdl-card {
    width: 100%;
    height: 320px;
  }
  .demo-card-square > .mdl-card__title {
    color: #fff;
    background:
      url('../assets/demos/dog.png') bottom right 15% no-repeat #46B6AC;
  }
</style>
 <div class="demo-grid-1 mdl-grid">
 
  <div class="mdl-cell mdl-cell--4-col">
          
    <div class="mdl-card mdl-shadow--2dp demo-card-square">
      <div class="mdl-card__title mdl-card--expand">
        <h2 class="mdl-card__title-text">Preguntas</h2>
      </div>
      <div class="mdl-card__supporting-text">
        El numero total de preguntas actualmente es de <%= data.preguntas %>
      </div>
      <div class="mdl-card__actions mdl-card--border">
        <a class="mdl-button mdl-button--colored mdl-js-button mdl-js-ripple-effect">
          View Updates
        </a>
      </div>
    </div>
       
  </div>
  
  <div class="mdl-cell mdl-cell--4-col">
      
    <div class="mdl-card mdl-shadow--2dp demo-card-square">
      <div class="mdl-card__title mdl-card--expand">
        <h2 class="mdl-card__title-text">Pistas</h2>
      </div>
      <div class="mdl-card__supporting-text">
        El numero total de comentarios actualmente es de <%= data.comentarios %>
      </div>
      <div class="mdl-card__actions mdl-card--border">
        <a class="mdl-button mdl-button--colored mdl-js-button mdl-js-ripple-effect">
          View Updates
        </a>
      </div>
    </div>
      
  </div>
  <div class="mdl-cell mdl-cell--4-col">
      
    <div class="mdl-card mdl-shadow--2dp demo-card-square">
      <div class="mdl-card__title mdl-card--expand">
        <h2 class="mdl-card__title-text">Promedio de pistas</h2>
      </div>
      <div class="mdl-card__supporting-text">
        El total promediado de todas las pistas es de <%= data.promedio %>
      </div>
      <div class="mdl-card__actions mdl-card--border">
        <a class="mdl-button mdl-button--colored mdl-js-button mdl-js-ripple-effect">
          View Updates
        </a>
      </div>
    </div>
      
  </div>
</div>


<div class="demo-grid-1 mdl-grid">
 
  <div class="mdl-cell mdl-cell--4-col">
          
    <div class="mdl-card mdl-shadow--2dp demo-card-square">
      <div class="mdl-card__title mdl-card--expand">
        <h2 class="mdl-card__title-text">Preguntas comentadas</h2>
      </div>
      <div class="mdl-card__supporting-text">
        El total de preguntas con pistas es de <%= data.con_coment %>
      </div>
      <div class="mdl-card__actions mdl-card--border">
        <a class="mdl-button mdl-button--colored mdl-js-button mdl-js-ripple-effect">
          View Updates
        </a>
      </div>
    </div>
       
  </div>
  
  <div class="mdl-cell mdl-cell--4-col">
      
    <div class="mdl-card mdl-shadow--2dp demo-card-square">
      <div class="mdl-card__title mdl-card--expand">
        <h2 class="mdl-card__title-text">Preguntas sin comentar</h2>
      </div>
      <div class="mdl-card__supporting-text">
        En este momento hay <%= data.sin_coment %> preguntas sin pistas
      </div>
      <div class="mdl-card__actions mdl-card--border">
        <a class="mdl-button mdl-button--colored mdl-js-button mdl-js-ripple-effect">
          View Updates
        </a>
      </div>
    </div>
      
  </div>
</div>